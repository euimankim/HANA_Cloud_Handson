1. Grant Privilidge : DBADMIN

GRANT AFL__SYS_AFL_AFLPAL_EXECUTE to devuser;
GRANT "sap.pa.apl.base.roles::APL_EXECUTE" to devuser;

2. Create table script : DEVUSER

CREATE COLUMN TABLE IRIS_DATA_TRAIN_TBL (
	 ID INTEGER,
	 SEPALLENGTHCM DOUBLE,
	 SEPALWIDTHCM DOUBLE,
	 PETALLENGTHCM DOUBLE,
	 PETALWIDTHCM DOUBLE,
	 SPECIES NVARCHAR(15));

CREATE COLUMN TABLE IRIS_DATA_TEST_TBL (
	 ID INTEGER,
	 SEPALLENGTHCM DOUBLE,
	 SEPALWIDTHCM DOUBLE,
	 PETALLENGTHCM DOUBLE,
	 PETALWIDTHCM DOUBLE,
	 SPECIES NVARCHAR(15));

CREATE COLUMN TABLE OZONERATE (
	 ID INTEGER NOT NULL,
	 OBSERVE_DATE DATE,
	 RATE NUMERIC(15,2));

